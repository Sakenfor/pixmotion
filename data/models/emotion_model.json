{
  "name": "curated_emotion_classifier",
  "version": "1.0",
  "features": ["brightness", "warmth", "contrast"],
  "labels": [
    {
      "id": "joyful",
      "tag": "emotion:joyful",
      "extras": ["subject:person", "palette:warm"]
    },
    {
      "id": "melancholy",
      "tag": "emotion:melancholy",
      "extras": ["subject:person", "palette:cool"]
