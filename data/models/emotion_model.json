  "weights": {
    "joyful": {
      "bias": -0.25,
      "weights": [3.0, 1.8, 0.5]
    },
    "melancholy": {
      "bias": 0.35,
      "weights": [-2.6, -2.1, -0.6]
    },
    "neutral": {
      "bias": 0.05,
      "weights": [0.1, -0.05, -0.1]
    }
  }
